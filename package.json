{
	"name": "previewfiles",
	"displayName": "PreviewFiles",
	"description": "",
	"icon": "icon.png",
	"version": "1.0.1",
	"publisher": "MaayanAndEden",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other",
		"Themes"
	],
	"activationEvents": [
		"onCommand:extension.previewHover"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.previewHover",
				"title": "Preview Hover"
			},
			{
				"command": "previewHover.newTabCommand",
				"title": "newTabCommand"
			},
			{
				"command": "previewHover.cssFileEditorCommand",
				"title": "cssFileEditorCommand"
			},
			{
				"command": "previewHover.textFileEditorCommand",
				"title": "textFileEditorCommand"
			}
				
		]
	},
	
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/react": "16.8.24",
		"react": "16.13.1",
		"angular": "1.7.9",
		"@angular/core": "9.0.7",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
